{
  "production": {
    "use_env_variable": "DATABASE_URL",
    "dialect": "mysql",
    "dialectOptions": {
      "ssl": {
        "require": true,
        "rejectUnauthorized": false
      }
    }
  },
  "development": {
    "use_env_variable": "DATABASE_URL_DEV",
    "dialect": "mysql",
    "dialectOptions": {
      "ssl": {
        "require": true,
        "rejectUnauthorized": false
      }
    }
  },
  "local": {
    "use_env_variable": "DATABASE_URL_LOCAL",
    "dialect": "mysql",
    "dialectOptions": {}
  }
}
